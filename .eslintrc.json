{
    "root": true,
    "extends": "babel",
    "plugins": [
        "prettier"
    ],
    "rules": {
        "curly": [
            "error",
            "multi-line"
        ],
        "prettier/prettier": "error",
        "no-case-declarations": "error"
    },
    "env": {
        "node": true
    }
}